{
  "manifest_version": 3,
  "name": "Bookmark Saver",
  "version": "1.0",
  "description": "Save bookmarks to your personal API",
  "permissions": ["activeTab", "storage", "scripting"],
  "host_permissions": [
    "https://saves.huntsyea.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}